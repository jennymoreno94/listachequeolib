{
  "compilerOptions": {
  "allowSyntheticDefaultImports": true,
  "declaration": true,
  "esModuleInterop": true, 
   "jsx": "react",
   "lib": [
    "es5",
    "es6",
    "dom"
  ],
  "module": "esnext", 
   "moduleResolution": "node",
   "outDir": "./dist", 
   "resolveJsonModule": true, 
   "sourceMap": false,
   "strict": true,
   "target": "es5", 
   "skipLibCheck": true  
  },
  "include": ["src"]
}